{
    "name": "Google Classroom Redirects",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "tabs"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://classroom.google.com/*"
            ],
            "js": [
                "background.js"
            ]
        }
    ]
}